<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/near_by"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/Hid"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:layout_width="142dp"
        android:layout_height="141dp"
        android:layout_above="@+id/textView10"
        android:layout_alignStart="@+id/Rid"
        android:layout_weight="1"
        android:alpha="0.5"
        android:scaleType="fitXY"
        android:src="@drawable/hotel_logo_android"
        android:visibility="visible" />

    <Button
        android:id="@+id/CLid"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:layout_width="142dp"
        android:layout_height="130dp"
        android:layout_alignBottom="@+id/Hid"
        android:layout_alignStart="@+id/Cid"
        android:layout_weight="1"
        android:alpha="0.5"
        android:scaleType="fitXY"
        android:src="@drawable/clothes_logo_android"
        android:visibility="visible" />


    <Button
        android:id="@+id/Rid"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:layout_width="142dp"
        android:layout_height="147dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="25dp"
        android:layout_weight="1"
        android:alpha="0.5"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/res_logo2_android" />

    <Button
        android:id="@+id/Cid"
        style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
        android:layout_width="142dp"
        android:layout_height="152dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/Rid"
        android:layout_marginEnd="27dp"
        android:layout_weight="1"
        android:alpha="0.5"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Rid"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/cafee_logo_android" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView12"
        android:layout_marginEnd="76dp"
        android:text="Cafees"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/CLid"
        android:layout_marginStart="84dp"
        android:layout_marginTop="-65dp"
        android:text="Hotels"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="36dp"
        android:layout_marginEnd="62dp"
        android:text="Clothes"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="63dp"
        android:layout_marginTop="186dp"
        android:text="Resturants"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:visibility="invisible" />


</RelativeLayout>